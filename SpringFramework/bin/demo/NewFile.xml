<?xml version="1.0" encoding="UTF-8"?>

<beans  xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:p="http://www.springframework.org/schema/p" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	
	<bean id="studentbean" class="demo.Student">  
		<property name="name" value="Vanathi"></property>  
		<property name="id" value='1' ></property>
		<property name="a" ref="addressbean"></property> 
		
	</bean>    
	<bean id="addressbean" class="demo.Address">
		<property name="city" value="Madurai"></property> 
		<property name="state" value="Tamilnadu"></property>
		<property name="location" value="India"></property>
	</bean>
	
</beans>